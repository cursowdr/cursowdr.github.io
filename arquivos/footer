require(['core/str','jquery','core/config'],function(str,$,config){var checkFunction=function(){var ads=$('#moodlecloud_ad');if(ads.length&&ads.height()===0&&!ads.data('notified')){YUI().use('moodle-core-notification-alert',function(){str.get_strings([{key:'adunblock_title',component:'theme_boost'},{key:'adunblock_message',component:'theme_boost'}]).done(function(strings){new M.core.alert({'title':strings[0],message:strings[1],width:'500px'});var settings={sesskey:config.sesskey};$.post(config.wwwroot+'/theme/boost/adsblocked.php',settings);ads.data('notified',!0)})})}};setTimeout(checkFunction,5000)})